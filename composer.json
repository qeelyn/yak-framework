{
  "name": "qeelyn/yak-framework",
  "description": "yak module message service",
  "repositories": [
    {
      "type": "git",
      "url":"https://github.com/tsingsun/yii2-framework.git"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "yiisoft/yii2": "~2.0.12"
  },
  "require-dev": {
    "phpunit/phpunit":"~4.8"
  },
  "autoload": {
    "psr-4": {
      "yak\\framework\\": "/src"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "yii\\composer\\Installer::postCreateProject"
    ]
  },
  "suggest":{
    "malkusch/lock":"if use idGenerate feature and the yii mutex is not satisfied,this lib is helpfull"
  }
}
